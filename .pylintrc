[VARIABLES]
#dummy-variables-rgx=([^_]+_)|dummy
dummy-variables-rgx=_|dummy
good-names=i,j,x,y,z
#additional-builtins=_

[MESSAGES CONTROL]
disable=
    logging-format-interpolation,
    no-else-return,
    # __main__.py files are too similar
    duplicate-code,
load-plugins=
    pylint.extensions.no_self_use

[SIMILARITIES]
ignore-imports = yes
